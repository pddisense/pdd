---
# Empty YAML front matter.
---
[
  {% assign pages = site.pages | where_exp:"page", "page.layout == 'docs'" %}
  {% for entry in pages %}
  {
    "title": {{ entry.title | jsonify }},
    "url": {{ entry.url | jsonify }},
    "content": {{ entry.content | strip_html | strip_newlines | jsonify }}
  }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
